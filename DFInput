public class DFInput {
	private String fruitName;
	private String fruitAbility;
	private String fruitType;

	public DFInput (String name, String ability, String type) {
		fruitName = name;
		fruitAbility = ability;
		fruitType = type;
	}

	public boolean contains (String dFruit) {
		if (dFruit.equals(fruitAbility)) {
			return true;
		}
		else {
			return false;
		}
	}
	
	public String toString () {
		return "Fruit Name: " + fruitName + "\n Fruit Ability: " + fruitAbility + "\n Fruit Type: " + fruitType;
	}

}
