import java.util.ArrayList;
import java.util.Scanner;

public class DevilFruitDatabase {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		ArrayList<DFInput> devilFruit = new ArrayList<>();
		devilFruit.add(new DFInput("Gomu Gomu no Mi", "Rubber", "Paramecia"));
		devilFruit.add(new DFInput("Bara Bara no Mi", "Chop", "Paramecia"));
		devilFruit.add(new DFInput("Moku Moku no Mi", "Smoke", "Logia"));
		devilFruit.add(new DFInput("Sube Sube no Mi", "Smooth", "Paramecia"));
		devilFruit.add(new DFInput("Hiso Hiso no Mi", "Whisper", "Paramecia"));
		devilFruit.add(new DFInput("Kama Kama no Mi", "Sickle", "Paramecia"));
		devilFruit.add(new DFInput("Bomu Bomu no Mi", "Bomb", "Paramecia"));
		devilFruit.add(new DFInput("Kilo Kilo no Mi", "Kilo", "Paramecia"));
		devilFruit.add(new DFInput("Doru Doru no Mi", "Wax", "Paramecia"));
		devilFruit.add(new DFInput("Baku Baku no Mi", "Munch", "Paramecia"));
		devilFruit.add(new DFInput("Ushi Ushi no Mi: Model Bison", "Bison", "Zoan"));
		devilFruit.add(new DFInput("Hito Hito no Mi", "Human", "Zoan"));
		devilFruit.add(new DFInput("Mane Mane no Mi", "Copy", "Paramecia"));
		devilFruit.add(new DFInput("Mera Mera no Mi", "Flame", "Logia"));
		devilFruit.add(new DFInput("Tori Tori no Mi: Model Falcon", "Falcon", "Zoan"));
		devilFruit.add(new DFInput("Hana Hana no Mi", "Bloom", "Paramecia"));
		devilFruit.add(new DFInput("Suna Suna no Mi", "Sand", "Logia"));
		devilFruit.add(new DFInput("Mogu Mogu no Mi", "Mole", "Zoan"));
		devilFruit.add(new DFInput("Inu Inu no Mi: Model Dachshund", "Dachshund", "Zoan"));
		devilFruit.add(new DFInput("Supa Supa no Mi", "Slice", "Paramecia"));
		devilFruit.add(new DFInput("Toge Toge no Mi", "Spike", "Paramecia"));
		devilFruit.add(new DFInput("Inu Inu no Mi: Model Jackal", "Jackal", "Zoan"));
		devilFruit.add(new DFInput("Ori Ori no Mi", "Cage", "Paramecia"));
		devilFruit.add(new DFInput("Bane Bane no Mi", "Spring", "Paramecia"));
		
		int dfIndex = 0;
		System.out.println("Input a fruit ability: ");
		String dFruit = scan.nextLine();
		while (!(devilFruit.get(dfIndex)).contains(dFruit)) {
			dfIndex++;
		}
		System.out.println(devilFruit.get(dfIndex));
		scan.close();
	}
}
